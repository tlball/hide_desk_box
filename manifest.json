{
  "manifest_version": 2,
  "name": "Continuity Desk Tweaks",
  "version": "0.0.3",
  "description": "UI Salesforce Desk modifications to make Desk easier to use for Continuity employees.",
  "icons": {
    "16": "images/desk16.png",
    "48": "images/desk48.png",
    "128": "images/desk128.png"
  },
  "content_scripts" : [{
    "matches": ["https://continuitycontrol.desk.com/*"],
    "js" : ["jquery.min.js","myScript.js"],
    "run_at": "document_end"
  }]
}
